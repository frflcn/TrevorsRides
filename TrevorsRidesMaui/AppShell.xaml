<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TrevorsRidesMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TrevorsRidesMaui"
    Shell.FlyoutBehavior="Flyout">
    <FlyoutItem Title="MainPage">
        <Tab>
            <ShellContent
                Title="Welcome To Trevor's Rides"
                ContentTemplate="{DataTemplate local:MainPage}"
                Route="MainPage" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="MapTest">
        <Tab>
            <ShellContent
                Title="Welcome To Trevor's Rides"
                ContentTemplate="{DataTemplate local:MapTestPage}"
                Route="MapTestPage" />
        </Tab>
    </FlyoutItem>
    

</Shell>
